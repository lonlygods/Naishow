
.big {
	width: 100%;
	height: 1400rpx;
	background-color: #F9F9F9;
	position: absolute;
	top: 0;
}
.headhome {
	width: 100%;
	height: 80rpx;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	line-height: 80rpx;
}
.homeleft {
	margin: 0 0 0 3%;
}
.homeright {
	margin: 0 3% 0 0;
}
.commditylist {
	width: 95%;
	height: 240rpx;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	margin: 3% 2.5%;
}
.bon button {
	color: #FFFFFF;
	background-color: #69C248;
	border-radius: 40rpx;
	font-weight: bold;
	border: 0;
	height: 80rpx;
}
.bon price {
	color: #69C248;
	font-weight: bold;
}
.title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	color: #5D5D5E;
	font-size: 36rpx;
	margin: 5%;
}
.msg {
	width: 100%;
	height: 240rpx;
	background-color: #FFFFFF;
	margin: 3% 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 34rpx;
}
.msg1 {
	width: 35%;
	height: 220rpx;
}
.msgbig {
	width: 90%;
	height: 220rpx;
	margin: 0 5%;
	border-radius: 20rpx;
}
.right2 {
	width: 65%;
}
.msgsize {
	width: 90%;
	font-size: 32rpx;
	margin: 0 0 0 5%;
}
.price_num {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 15% 0 0 0;
}
.van-swipe-cell__right {
	display: inline-block;
	width: 65px;
	height: 44px;
	font-size: 15px;
	line-height: 130px;
	color: #fff;
	text-align: center;
	font-weight: bold;
	background-color: #f44;
}
.check{
	margin: 12% 2%;
}
.checkde{
	margin: 0 4%;
}

